{
  "name": "figma-plugin-copy-prototype-link",
  "version": "2.2.0",
  "description": "Figma plugin that provides an easy way to copy a Prototype link to the clipboard without having to switch to the Presentation mode.",
  "main": "dist/index.js",
  "scripts": {
    "dev": "npm run watch",
    "build": "webpack --mode=production",
    "watch": "webpack --mode=development --watch",
    "bump": "node_modules/.bin/bump prompt --tag --all --push --commit 'v%s'",
    "patch": "node_modules/.bin/bump patch --tag --all --push --commit 'v%s'",
    "version": "echo 'Building before bump...' && npm run build"
  },
  "keywords": [
    "figma",
    "plugin",
    "copy",
    "prototype",
    "link"
  ],
  "author": "Max Martynov <martynov.max.s@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maxmartynov/figma-plugin-copy-prototype-link.git"
  },
  "bugs": {
    "url": "https://github.com/maxmartynov/figma-plugin-copy-prototype-link/issues"
  },
  "homepage": "https://github.com/maxmartynov/figma-plugin-copy-prototype-link/#readme",
  "dependencies": {
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@figma/plugin-typings": "^1.19.2",
    "css-loader": "^3.5.2",
    "html-webpack-inline-source-plugin": "0.0.10",
    "html-webpack-plugin": "^3.2.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^6.0.4",
    "typescript": "^3.8.3",
    "url-loader": "^2.1.0",
    "version-bump-prompt": "^6.0.3",
    "vue-loader": "^15.7.1",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.46.0",
    "webpack-cli": "^4.6.0"
  }
}
